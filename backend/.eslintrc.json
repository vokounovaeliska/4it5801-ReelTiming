{
  "extends": ["../.eslintrc.cjs"],
  "overrides": [
    {
      "files": ["*.ts"],
      "rules": {
        "react/no-is-mounted": "off",
        "@typescript-eslint/no-explicit-any": "warn"
      }
    },
    {
      "files": ["*.tsx"],
      "rules": {
        "react/no-is-mounted": "error",
        "@typescript-eslint/no-explicit-any": "warn"
      }
    }
  ]
}
